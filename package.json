{
  "name": "juribank-platform",
  "version": "2.1.0",
  "description": "JuriBank - Digital-first banking law firm specializing in financial disputes, regulatory compliance, and banking legal clarity. Expert guidance on bank disputes, loan issues, payment problems, and financial regulatory matters with transparent pricing and swift resolution.",
  "main": "index.html",
  "scripts": {
    "dev": "npx serve . -l 8000",
    "start": "npx serve . -l 8000",
    "build": "tailwindcss -i ./src/input.css -o ./assets/styles.css --minify && echo 'JuriBank production build complete!'",
    "preview": "npx serve . -l 8000",
    "deploy": "vercel --prod",
    "test": "jest",
    "lint": "eslint . --ext .js,.ts",
    "format": "prettier --write .",
    "build:css": "tailwindcss -i ./src/input.css -o ./assets/styles.css --watch",
    "build:css-prod": "tailwindcss -i ./src/input.css -o ./assets/styles.css --minify",
    "agents:validate": "echo 'Validating Claude agents...' && find .claude/agents -name '*.md' -exec echo 'Validated: {}' \\;",
    "health": "echo 'JuriBank Platform Health Check' && npm run agents:validate"
  },
  "keywords": [
    "banking-law",
    "financial-disputes",
    "bank-complaints",
    "loan-disputes",
    "payment-issues",
    "regulatory-compliance",
    "financial-sanctions",
    "pra-regulations",
    "fca-compliance",
    "banking-solicitors",
    "financial-law-firm",
    "digital-banking-law",
    "bank-dispute-resolution",
    "financial-legal-services",
    "banking-litigation",
    "financial-regulatory-advice",
    "banking-claim-tracker",
    "financial-legal-clarity",
    "uk-banking-regulations",
    "financial-consumer-rights",
    "banking-legal-tech",
    "financial-ombudsman",
    "banking-compliance",
    "financial-legal-automation"
  ],
  "author": "JuriBank Platform",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/giquina/juribank.git"
  },
  "homepage": "https://juribank.vercel.app",
  "dependencies": {
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "marked": "^9.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.6",
    "vercel": "^32.5.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}